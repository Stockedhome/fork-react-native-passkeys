{
  "name": "@stockedhome/react-native-passkeys",
  "version": "1.0.0",
  "description": "A library for using (webauthn) passkeys on iOS, Android & web with a single api",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "expo-module": "expo-module",
    "open:ios": "open -a \"Xcode\" example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "prepare": "pwsh -f expo-module-prepare.ps1"
  },
  "keywords": [
    "react-native",
    "react-native-module",
    "native-module",
    "expo-module",
    "module",
    "expo",
    "auth",
    "webauthn",
    "rn-webauthn",
    "react-native-webauthn",
    "ReactNativeWebAuthn",
    "passkey",
    "passkeys",
    "rn-passkey",
    "rn-passkeys",
    "react-native-passkey",
    "react-native-passkeys",
    "ReactNativePasskey",
    "ReactNativePasskeys"
  ],
  "repository": "https://github.com/peterferguson/react-native-passkeys",
  "bugs": {
    "url": "https://github.com/peterferguson/react-native-passkeys/issues"
  },
  "author": "Peter Ferguson <peterferguson95@gmail.com> (https://github.com/peterferguson)",
  "license": "MIT",
  "homepage": "https://github.com/peterferguson/react-native-passkeys#readme",
  "exports": {
    "./src/*": {
      "import": "./src/*.ts",
      "require": "./src/*.js",
      "types": "./src/*.js"
    },
    ".": {
      "types": "./src/index.ts",
      "import": "./build/index.js",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./build/*.js",
      "require": "./build/*.js"
    }
  },
  "dependencies": {
    "@hexagon/base64": "^1.x.x",
    "@simplewebauthn/browser": "^10.0.x",
    "expo-constants": "16.x.x",
    "zod": "3.x.x"
  },
  "devDependencies": {
    "@biomejs/biome": "1.2.2",
    "@changesets/cli": "^2.26.2",
    "@simplewebauthn/server": "^10.0.0",
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@types/react": "^18.0.25",
    "expo-module-scripts": "^3.4.1",
    "expo-modules-core": "^1.11.9",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
